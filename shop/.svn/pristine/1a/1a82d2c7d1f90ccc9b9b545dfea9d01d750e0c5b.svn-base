.F_cities .F_ins{position:fixed;z-index:10;top:40%;left:50%;width:100px;height:100px;margin:-50px 0 0 -50px;border-radius:8px;background:rgba(0,0,0,.2);color:#fff;text-align:center;font-size:200%;line-height:100px;pointer-events:none;text-shadow:0 1px 1px #555;}
section{margin:0 16px;/* border-top:90px solid transparent;box-sizing:border-box; */}
.F_cities nav{position:fixed;z-index:9;top:70px;right:0px;height:100%;}
.F_cities nav div{}
.F_cities nav a{display:block;text-align:center;font-size:90%;line-height:1.3;-webkit-tap-highlight-color:rgba(0,0,0,0);font-family:Arial;font-weight:bold;color:#638c0b;width:60px;}
.F_cities .F_RList li{padding:0;}
.F_cities .F_RList li a{display:block;padding:20px 2em;color:#444;}
.F_cities .F_RList li:first-of-type a{border-radius:20px 20px 0 0;}
.F_cities .F_RList li:last-of-type a{border-radius:0 0 20px 20px;}
/* Separate shadow */
.Shadow_box{position:relative;margin:8px auto 0;padding-bottom:0;}
.Shadow_box:before,.Shadow_box:after{content:"";display:block;position:absolute;z-index:-1;bottom:2px;width:47%;height:4px;background:#4c4c4c;-webkit-box-shadow:0 1px 3px 3px rgba(0,0,0,0.5);}
.Shadow_box:before{left:10px;-webkit-transform:rotate(-1.4deg);}
.Shadow_box:after{right:10px;-webkit-transform:rotate(1.4deg);}
.Shadow_box:before,.Shadow_box:after{z-index:1;bottom:8px;width:47.6%;}
.Shadow_box img{position:relative;z-index:2;}
/* 启动页 */
.F_startup{position:absolute;top:50%;width:100%;margin-top:-210px;text-align:center;}
#loadingBox{position:fixed;z-index:100;top:50%;left:50%;width:210px;height:100px;margin:-60px auto auto -135px;padding:10px 30px;border: 1px solid #2A2A2A;color:#fff;text-align:center;border-radius:10px;background:rgba(0,0,0,.5);}
#loadingBox p{}
#loadingBox i{position:absolute;z-index:9;top:70%;left:50%;margin:-25px 0 0 -25px;display:inline-block;width:32px;height:32px;border:6px dotted #000;border-color:rgba(255,255,255,.1) rgba(255,255,255,.2) rgba(255,255,255,.4) rgba(255,255,255,.5);border-radius:300px;-webkit-animation:loadforever 2.5s infinite linear;}
@-webkit-keyframes loadforever{0%{-webkit-transform:rotate(0deg);}100%{-webkit-transform:rotate(360deg);}}
/* 提示框 */
.PointTxt{position:absolute;z-index:100;top:50%;left:50%;width:200px;margin-top:-35px;margin-left:-130px;padding:10px 30px;color:#fff;text-align:center;opacity:0.85;background:-webkit-gradient(linear,0 100%,0 20%,from(#666),to(#222));
border:2px solid #eee;border-radius:8px;box-shadow:0 4px 8px #000;text-shadow:0 2px 1px #000;}
.PointTxt .Close{overflow:hidden;position:absolute;right:-10px;top:-10px;width:24px;height:24px;background:url(../images/cancel_001.png) no-repeat;border-radius:10px;-webkit-user-select:none;color:transparent;text-shadow:none;}
.PointTxt h2{height:auto;margin:-10px -30px 0;padding:8px 0;border-bottom:1px solid #fff;text-align:center;background:-webkit-gradient(linear, left top, left bottom, from(#444), to(black));border-radius:6px 6px 0 0;}
.PointTxt p{margin:8px -12px;text-align:left;}
.PointTxt *:last-child{margin-bottom:0;}
#loadingBox{zoom:.5;font-size:120%;}
.PointTxt{font-size:12px;text-shadow:none;}
.PointTxt h2{margin:-10px -30px 0;padding:0;text-shadow:none;line-height:30px;}
/* 省略号不换行 */
.nowrap{overflow:hidden;white-space:nowrap;text-overflow:ellipsis;}
