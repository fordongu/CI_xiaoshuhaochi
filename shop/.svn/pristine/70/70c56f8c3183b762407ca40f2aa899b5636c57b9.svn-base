.views_main_wrap dl:nth-child(1),
.views_main_wrap dl:nth-child(2){
    margin-bottom: 10px;
}
.views_main_wrap dl:nth-child(1){
    margin-right: 10px;
}

.views_main_wrap dl:nth-child(4){
    margin: 0 10px;
}*/
.views_main_wrap dl:nth-child(1) dt{
    position: absolute;
    left: 5px;
    top: 5px;
}
.views_main_wrap dl:nth-child(1) dd{
    position: absolute;
    right: 5px;
    top: 5px;
    height: 300px;
    width: 340px;
    padding:0 5px;
    box-sizing:border-box;
}
.views_main_wrap dl:nth-child(1) li:nth-child(1) img:nth-child(1){
    width: 320px;
    height: 200px;
    display: inline-block;
}
.views_main_wrap dl:nth-child(1) dt li:nth-child(2){
    line-height: 40px;
    margin-top:10px;
}
.views_main_wrap dl:nth-child(1) dt li:nth-child(3) img:nth-of-type(1){
    margin-right: 10px;
}
.views_main_wrap dl:nth-child(1) li:nth-child(3) img:nth-child(1),
.views_main_wrap dl:nth-child(1) li:nth-child(3) img:nth-child(2){
    width: 80px;
    height: 50px;
    display: inline-block;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(1) h3{
    margin: 10px 0;
    color: #230304;
    font-size: 24px!important;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(2) ol{
    height:10px;
    margin: 10px 0 22px;
    color: #230304;
    font-size: 14px!important;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(2) ol li{
    float: left;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(2) ol li:nth-child(2){
    /*margin-left: 30px;*/
    margin-top:10px;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(3){
    width: 100%;
    height: 120px;
    overflow:hidden;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(3) p{
    width: 100%;
    height: 100%;
    overflow: hidden;
     word-break:break-all;
    word-wrap:break-word;
    line-height: 25px;
} 
.views_main_wrap dl:nth-child(1) dd li:nth-child(4){
    position: absolute;
    bottom: 0;
    left: 10;
    color: #ff6000;
    width: 93%;
    line-height: 30px;
    font-size: 18px!important;
}
.views_main_wrap dl button{
    width: 100px;
    height: 32px;
    border-radius: 2px;
    border:1px solid #242424;
}
.views_main_wrap dl button:hover{
    background: #ff6000;
    color: #fff;
    border:1px solid #ff6000;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(4) button{
    position: absolute;
    right: 0;
    bottom: 0;
}
.views_main_wrap dl:nth-child(1) dd li:nth-child(4) small{
    font-size: 14px!important;
    /*margin-right: 10px;*/
    display: inline-block;
}

.views_main_wrap dl:nth-child(2){
    /*margin-left: 10px!important;
    margin-top: 0!important;*/
}

.views_main_wrap .views_main_content_small{
