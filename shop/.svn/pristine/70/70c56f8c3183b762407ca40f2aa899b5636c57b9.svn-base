}
.views_main_wrap .views_main_index_carouse ul{
    position: absolute;
    top: 0;
    height: 100%;
}
.views_main_wrap .views_main_index_carouse ul li{
    width: 1040px;
    height: 100%;
    float: left;
    padding:10px;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
}
.views_main_wrap .views_main_index_carouse ul div{
    width: 510px;
    float: right;
}
.views_main_wrap .views_main_index_carouse ul div span{
    margin-top: 10px;
    display: inline-block;
}
.views_main_wrap .views_main_index_carouse ul img{
    width: 500px;
    height: 100%;
    display: inline-block;
    vertical-align: bottom;
}
.views_main_wrap .views_main_index_carouse ul div h5{
    
}
.views_main_wrap .views_main_index_carouse ul div span:last-child{
    margin-top: 20px;
    height: 200px!important;
    overflow: hidden;
    display: inline-block;
    text-align: center;
    font-size: 14px;
    word-break:break-all;
    word-wrap:break-word; 
    line-height: 25px;
}
.views_main_wrap .views_main_index_carouse ol{
    width: 100px;
    height: 40px;
    position: absolute;
    left: 50%;
    bottom: 0;
    margin-left: -50px;
    background: #2a2a34;
    filter:alpha(opacity:80);
    opacity:0.8;
    text-align: center;
    cursor: default;
}
.views_main_wrap .views_main_index_carouse ol li{
    float: left;
    margin: 10px 0;
    padding-left: 11px;
}
.views_main_wrap .views_main_index_carouse ol .normal a{
    height: 5px;
    width: 5px;
    border-radius: 10px;
    background: #fff;
    border:1px solid transparent;
    display: inline-block;
}
.views_main_wrap .views_main_index_carouse ol .active a{
    border:1px solid #fff;
    background: transparent;
}
