 /*views_main_header Begin*/
 .views_main_header{
    position: fixed;
    left: 0;
    top: 0;
    width: 200px;
    text-align: center;
    color: #9FA0A4;
    background: #373641;
    z-index: 2;
    opacity: 0.8;
    filter:alpha(opacity:80);
 }
 .views_main_header a{
    display: inline-block;
    color: #9FA0A4;
 }
 .views_main_header ul{
    width: 100%;
    height: 100%;
 }
 .views_main_header ul > li{
    width: 160px;
    margin: 0 auto;
    border-bottom: 1px solid #9FA0A4;
 }
 .views_main_header ul > li:nth-child(2) ol{
    width: 100%;
    display: inline-block;
 }
 .views_main_header ul > li:nth-child(3) ol{
    width: 100%;
    display: inline-block;
 }
 .views_main_header ul > li ol li{
    line-height: normal;
    cursor: pointer;
 }
.views_main_header ul > li:nth-child(1) span{
    width: 82px;
    height: 82px;
    background: url('../icon/logo.png');
    display: inline-block;
    position: relative;
    top: 50%;
    cursor: pointer;
 }
 .views_main_header ul > li:nth-child(2),
 .views_main_header ul > li:nth-child(3),
 .views_main_header ul > li:nth-child(4){
    padding:10px 0;
    box-sizing:border-box;
 }
 .views_main_header ul > li:nth-child(2) ol li{
    position: relative;
    text-align: center;
    width: 100%;
 }
 .views_main_header ul > li:nth-child(2) ol li:nth-child(1) span:nth-child(1){
    width: 30px;
    height: 30px;
    display: inline-block;
    vertical-align: middle;
    margin-bottom: 10px;
 }
 .views_main_header ul > li:nth-child(2) ol li:nth-child(1) span:nth-child(1){
    background: url('../icon/index-icon2.png') 0 0 no-repeat;
    background-size: 60px 120px;
 }
 .views_main_header ul > li:nth-child(2) ol li:nth-child(1):hover span{
    background: url('../icon/index-icon2.png') -30px 0 no-repeat;
    background-size: 60px 120px;
 }
 .views_main_header ul > li:nth-child(2) ol li:nth-child(2) span{
    display: inline-block;
    width: 100%;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(1) span:nth-child(3),
 .views_main_header ul > li:nth-child(3) ol li:nth-child(2) span:nth-child(3){
    display: inline-block;
    width: 110px;
    height: 15px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space:normal;
    text-align: center;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(2){
    margin: 24px 0;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(1) span:nth-child(1),
 .views_main_header ul > li:nth-child(3) ol li:nth-child(2) span:nth-child(1){
    width: 5px;
    height: 5px;
    border:1px solid #9FA0A4;
    border-radius: 10px;
    display: inline-block;
    margin-right: 5px;
 }

 .views_main_header ul > li:nth-child(3) ol .active span:nth-child(1){
    width: 6px;
    height: 6px;
    background: #9FA0A4;
    border-radius: 10px;
    display: inline-block;
 }
 .views_main_header ul > li:nth-child(3) ol .active span:nth-child(3){
    color: #00A538;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3){
    position: relative;
    width: 90px;
    height: 90px;
    background: #9FA0A4;
    margin-top: 40px;
    margin-left: 30px;
 }
 
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span{
    display: inline-block;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 45px;
    border-color: transparent;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(1){
    position: absolute;
    top: -1px;
    left: 0;
    border-top-color: #373641;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(2){
    position: absolute;
    left: -1px;
    border-left-color: #373641;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(3){
    position: absolute;
    right: -1px;
    border-right-color: #373641;
 }

 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(4){
    position: absolute;
    bottom: -1px;
    left: 0;
    border-bottom-color: #373641;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span i{
    display: inline-block;
    width: 30px;
    height: 30px;
    z-index: 30;
    cursor: pointer;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(1) i{
    position: absolute;
    top: -50px;
    left: -13px;
    background: url('../icon/index-icons.png') 0 -60px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(2) i{
    position: absolute;
    top: -15px;
    left: -55px;
    background: url('../icon/index-icons.png') 0 -150px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(3) i{
    position: absolute;
    top: -15px;
    right: -55px;
    background: url('../icon/index-icons.png') 0 -90px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(4) i{
    position: absolute;
    bottom: -50px;
    left: -13px;
    background: url('../icon/index-icons.png') 0 -120px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(1):hover i{
    background: url('../icon/index-icons.png') -30px -60px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(2):hover i{
    background: url('../icon/index-icons.png') -30px -150px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(3):hover i{
    background: url('../icon/index-icons.png') -30px -90px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(3) ol li:nth-child(3) span:nth-child(4):hover i{
    background: url('../icon/index-icons.png') -30px -120px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(4) ol li{
    height: 30px;
    margin: 10px 0;
 }
 .views_main_header ul > li:nth-child(5){
    padding: 10px 0;
 }
 .views_main_header ul > li:nth-child(5) span{
    width: 30px;
    height: 30px;
    margin: 0 14px;
    display: inline-block;
 }
 .views_main_header ul > li:nth-child(5) span:nth-child(1){
    background: url('../icon/index-icons.png') 0 -180px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(5) span:nth-child(2){
    background: url('../icon/index-icons.png') 0 -210px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(5) span:nth-child(1):hover{
    background: url('../icon/index-icons.png') -30px -180px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:nth-child(5) span:nth-child(2):hover{
    background: url('../icon/index-icons.png') -30px -210px no-repeat;
    background-size: 60px 240px;
 }
 .views_main_header ul > li:last-child{
    width: 100%;
    border-bottom: none;
 }
 .views_main_header ul > li:last-child li:nth-child(1),
 .views_main_header ul > li:last-child li:nth-child(3){
    padding:10px 0;
 }
 /*views_main_header End*/

 /*views_main Begin*/
.views_main_content{
    position: relative;
    padding-left: 200px;
    background: url('../images/bg.jpg') 0 0 no-repeat fixed;
    overflow-y:scroll;
}
.views_main_wrap{
    width: 1060px;
    height: 1000px;
    margin: 0 auto;
    padding: 40px 0;   
    opacity: 0.8;
    filter:alpha(opacity:80);
}
.views_main_wrap dl{
    float: left;
    width: 340px;
    height: 320px;
    padding:5px;
    box-sizing:border-box;
    -moz-box-sizing:border-box;
    -webkit-box-sizing:border-box;
    position: relative;
    background: #fff;
}
.views_main_wrap dl:nth-child(1){
    width: 690px!important;
}
.views_main_wrap dl img{
    cursor: pointer;
}
