
/*rest*/
html, body, div, p, ul,ol, li, dl, dt, dd, h1, h2, h3, h4, h5, h6, form, input, select, button, textarea, iframe, table, th, td { margin: 0; padding:0;}
div,dl,dt,dd,form,h1,h2,h3,h4,h5,h6,img,ol,ul,li,table,th,td,p,span,a{border:0;font-weight: normal;}
table{border-collapse: collapse;}
article,aside,dialog,figure,footer,header,legend,nav,section{display: block;}
img,input{border:none;vertical-align:middle;-webkit-appearance: none;outline: none;}
body{font-family:"Microsoft YaHei","微软雅黑",Tahoma,Arial,Helvetica,宋体;font-size:14px;overflow: hidden;background: #F0EEF4;cursor:default;}
html{overflow-y:scroll;}
ul,ol{list-style-type:none;}
th,td,input{font-size:14px;}
h1{ font-size:24px;}
h2{ font-size:20px;}
h3{ font-size:18px;}
h4{ font-size:16px;}
h5{ font-size:14px;}
h6{ font-size:12px;}button{border:none;cursor:pointer;font-size:12px;background-color:transparent;}
select{border-width:1px;_zoom:1;border-style:solid;padding-top:2px;font-size:12px;}
.clear{clear:both;font-size:1px;height:0;visibility:hidden;line-height:0;}
.clearfix:after{content:"";display:block;clear:both;}
.clearfix{zoom:1;}
a:link,a:visited{text-decoration:none;}
a:hover,a:active{text-decoration:none;cursor: pointer;}
.absolute{position:absolute;}
.relative{position:relative;}
.UnLine{text-decoration:underline;}
.DisunLine{text-decoration:none;}
.o-hidden{overflow: hidden;}
.none{display: none;}
.block{display: block;}
.t-l{text-align:left;}
.t-c{text-align:center;}
.t-r{text-align:right;}
.f-l{float:left;}
.f-r{float:right;}
.f-n{float:none;}
.ml-10{margin-left:10px}
.mr-10{margin-right:10px}
.mt-10{margin-top:10px}
.mb-10{margin-bottom:10px}
.pl-10{padding-left:10px}
.pr-10{padding-right:10px}
.pt-10{padding-top:10px}
.pb-10{padding-bottom:10px}
