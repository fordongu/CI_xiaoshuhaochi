



body,
html {
  margin: 0;
  font-family: "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
  -webkit-font-smoothing: antialiased;
  font-size: 62.5%;
}
.color-red {
  color: #ff316b !important;
}
.color-blue {
  color: #00bff3 !important;
}
body {
  background-color: black;
}
#event-poster {
  width: 100%;
}
#header {
  position: relative;
  min-height: 20rem;
  z-index: 0;
}
.header-logo {
  position: fixed;
  right: 2rem;
  top: 2rem;
  width: 4rem;
  height: 4rem;
  border-radius: 2rem;
  z-index: 0;
}
#event-title {
  position: relative;
  width: 100%;
  font-size: 2rem;
  line-height: 3rem;
  color: white;
  margin-bottom: 0.6rem;
  font-weight: bold;
  padding-bottom: 0.6rem;
  box-shadow: 0 1px 0 rgba(255, 255, 255, 0.2);
}
#jquery_jplayer_1 {
  position: absolute;
  width: 4rem;
  height: 4rem;
  bottom: 1.4rem;
  left: 2rem;
  background-color: rgba(0, 0, 0, 0.66);
  border-radius: 0.6rem;
}
#event-info {
  width: 100%;
  padding: 1.2rem 2rem 1.6rem 2rem;
  word-wrap: break-word;
  word-break: normal;
  box-sizing: border-box;
  color: white;
  font-size: 1.4rem;
}
#event-time-text,
#event-place-text {
  font-size: 1.4rem;
  line-height: 2.6rem;
  height: 2.6rem;
  color: #eee;
}
#event-rank {
  margin-top: 0.2rem;
}
.event-rank-icon {
  height: 2.5rem;
  width: 10rem;
}
#event-rank-icon img {
  width: 2rem;
  float: left;
}
#event-rank-icon #event-rank-text {
  float: left ;
}
#event-rank-text {
  position: relative;
  left: 1rem;
  bottom: 0.3rem;
  font-size: 1.4rem;
  line-height: 3rem;
  height: 3rem;
  color: #ffd687;
  font-family: "numberlight", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}
#event-rank-text small {
  font-size: 1rem;
  /*font-family: "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;*/
}
#event-description {
  width: 100%;
  padding: 0 2rem;
  white-space: pre;
  word-wrap: break-word;
  word-break: normal;
  box-sizing: border-box;
  padding-bottom: 10rem;
  color: #ccc;
  font-size: 1.4rem;
  line-height: 2.6rem;
}
.auction-panel-wrapper {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  width: 100%;
  height: 7rem;
  background-color: white;
}
#auction-panel-arrow {
  position: absolute;
  top: -1.6rem;
  width: 3rem;
  height: 1.2rem;
  left: 50%;
  margin-left: -1.5rem;
}
#auction-panel-arrow img {
  width: 100%;
}
.auction-panel {
  width: 100%;
  padding-bottom: 4rem;
  position: absolute;
  top: 0;
  background-color: white;
  z-index: 10;
  -webkit-user-select: none;
  -ms-user-select: none;
  user-select: none;
  -webkit-transform: translate3d(0, 0, 0);
  transform: translate3d(0, 0, 0);
  box-shadow: 0 40rem 0 white;
}
.auction-panel:before {
  content: "";
  width: 100%;
  height: 5rem;
  background-color: white;
  position: absolute;
  top: 100%;
}
.auction-panel small {
  font-size: 1.3rem;
  color: #808080;
}
.auction-panel-disable {
  /*overflow: hidden;*/
}
.auction-panel-disable:before {
  content: "";
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  height: 0;
  background-color: white;
}
.black-auction-panel,
#black-popup {
  display: none;
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  background-color: rgba(0, 0, 0, 0.72);
}
#black-popup {
  z-index: 20;
}
#auction-panel-header {
  height: 7rem;
  width: 100%;
  position: relative;
}
.auction-panel-header-major {
  font-size: 4.4rem;
  font-family: "numberlight", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
  width: 100%;
  height: 5.2rem;
  padding-top: 0;
  line-height: 5.6rem;
  text-align: center;
  position: absolute;
  top: 0;
}
.auction-panel-header-major small {
  margin: 0 0.5rem;
}
.auction-time-left-small {
  font-family: "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
  font-size: 1.3rem;
  line-height: 1.3rem;
  top: 4.6rem;
  color: #808080;
}
.auction-time-left-small small {
  margin: 0 0.3rem;
}
#auction-fail {
  font-size: 2.4rem;
}
#auction-win,
#auction-fail,
#auction-over {
  font-size: 2.4rem;
  font-weight: 600;
  height: 5.2rem;
  line-height: 5rem;
  top: 0.1em;
  color: #444;
}
#auction-time-left {
  /*display: none;*/
}
#auction-win {
  display: none;
}
#auction-fail {
  display: none;
}
#auction-over {
  display: none;
}
.auction-panel-header-minor {
  position: absolute;
  top: 4.8rem;
  width: 100%;
  text-align: center;
  font-size: 1.3rem;
  line-height: 1.3rem;
  color: #808080;
}
.auction-panel-header-minor span {
  color: #ff316b;
}
#auction-state {
  line-height: 5.2rem;
  top: 0;
}
#auction-state span {
  color: #00bff3;
  font-size: 3.2rem;
  font-family: "numberlight", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}
#auction-state small {
  color: #00bff3;
}
#auction-over-backtolist {
  color: #00bff3;
}
#auction-attract {
  /*display: none;*/
  top: 5.1rem;
}
#auction-bidders {
  display: none;
}
#auction-state {
  display: none;
}
#auction-win-inf {
  display: none;
}
#auction-fail-inf {
  display: none;
}
#auction-over-backtolist {
  display: none;
}
#auction-panel-price {
  position: relative;
  top: 1rem;
  box-sizing: border-box;
  padding: 0 3.6rem;
  height: 20rem;
  width: 100%;
}
#auction-panel-ticket {
  width: 100%;
  height: auto;
  display: block;
  background-color: #00bff3;
  padding: 1rem 0;
  margin-top: 1rem;
}
#auction-panel-ticket table {
  width: auto;
  max-width: 90%;
  position: relative;
  margin: 0 auto;
  background-color: #00bff3;
}
#auction-panel-ticket .ticket-info-head {
  position: relative;
  padding-top: 0.5rem;
  line-height: 2rem;
  font-size: 1.6rem;
  width: 7rem;
  text-align: right;
  vertical-align: text-top;
  color: white;
}
#auction-panel-ticket .ticket-info-body {
  padding-top: 0.5rem;
  padding-bottom: 0.5rem;
  padding-left: 1.6rem;
  height: 2rem;
  line-height: 2rem;
  font-size: 1.6rem;
  color: white;
}
#auction-panel-ticket tr:first-child td:first-child:after {
  content: "";
  position: absolute;
  top: -1.5rem;
  right: -1.2rem;
  width: 1rem;
  height: 1rem;
  border-radius: 0.5rem;
  background-color: white;
}
#auction-panel-ticket tr:last-child td:first-child:after {
  content: "";
  position: absolute;
  bottom: -1.5rem;
  right: -1.2rem;
  width: 1rem;
  height: 1rem;
  border-radius: 0.5rem;
  background-color: white;
}
#auction-panel-connect-inf {
  margin-top: 1.2rem;
  text-align: center;
  font-size: 1.3rem;
  line-height: 1.8rem;
  color: #444;
}
#auction-panel-price {
  /*display: none;*/
}
#auction-panel-ticket {
  display: none;
}
#auction-panel-connect-inf {
  display: none;
}
.auction-bid-state-wrapper {
  position: relative;
}
#auction-bid-state {
  display: inline-block;
  text-align: center;
  left: 1rem;
  height: 5.2rem;
  position: absolute;
  color: #00bff3;
  top: 0;
  left: -0.9rem;
}
#auction-bid-price {
  display: inline;
  font-size: 4.4rem;
  line-height: 5.2rem;
  font-family: "numberlight", "Helvetica Neue", Arial, Helvetica, Geneva, sans-serif;
}
#auction-bid-price small {
  color: #00bff3;
}
#auction-price-slider-container {
  position: relative;
  top: 5.2rem;
  width: 100%;
  box-sizing: border-box;
}
#auction-price-sliderbar {
  position: relative;
  top: 1rem;
  height: 0.3rem;
  background-color: #ddd;
  border-radius: 0.15rem;
}
#auction-price-sliderbar-forbidden {
  position: absolute;
  background-color: #00bff3;
  top: 0;
  bottom: 0;
  left: 0;
  right: 10%;
}
#auction-price-slider {
  position: absolute;
  height: 4.4rem;
  width: 4.4rem;
  top: -2.1rem;
  left: 0;
  margin-left: -2.2rem;
  z-index: 12;
}
#auction-price-slider-dot {
  position: absolute;
  height: 1.8rem;
  width: 1.8rem;
  left: 50%;
  margin-left: -0.9rem;
  top: 50%;
  margin-top: -0.9rem;
  background: -webkit-linear-gradient(top, #ffffff 0%, #f1f1f1 72%, #f5f5f5 100%) #ffffff;
  background: -moz-linear-gradient(top, #ffffff 0%, #f1f1f1 72%, #f5f5f5 100%) #ffffff;
  background: -ms-linear-gradient(top, #ffffff 0%, #f1f1f1 72%, #f5f5f5 100%) #ffffff;
  background: linear-gradient(to bottom, #ffffff 0%, #f1f1f1 72%, #f5f5f5 100%) #ffffff;
  border-radius: 0.9rem;
  box-shadow: 0 1px 4px rgba(0, 0, 0, 0.2), 0 0 1px rgba(0, 0, 0, 0.5), 0 0 3px rgba(255, 255, 255, 0.5) inset;
}
#price-tip {
  white-space: nowrap;
  position: absolute;
  width: 100%;
  top: 1.6rem;
  font-size: 1.3rem;
  color: #808080;
  z-index: 11;
}
#bid-start-price,
#retail-price {
  color: #00bff3;
}
